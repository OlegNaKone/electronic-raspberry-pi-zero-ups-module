                                 		*/
000028 e000
000029 bf00                      			OUTI	TCCR0A, 0<<COM0A1 | 0<<COM0A0 | 0<<COM0B1 | 0<<COM0B0 | 0<<WGM01 | 0<<WGM00
00002a e001
